<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>

    </style>

    <style name="FindYourPubTheme" parent="Theme.AppCompat.NoActionBar">
        <item name="android:colorBackground">@color/colorBackground</item>
        <item name="colorAccent">@android:color/holo_red_light</item>
        <item name="android:colorForeground">@color/foreground_material_dark</item>
    </style>

    <style name="FindYourPubTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="FindYourPubTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>
